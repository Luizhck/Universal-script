local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "✌️", HidePremium = false, SaveConfig = true, ConfigFolder = "white"})
local Tab = Window:MakeTab({
	Name = "animal simulator",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddButton({
	Name = "farma",
	Callback = function()
     lock = true
    while true do
    wait(0.1)
     if lock == true then
     game:GetService("ReplicatedStorage").Events.CoinEvent:FireServer()
		end
  	end
  	end    
})
Tab:AddButton({
	Name = "parar",
	Callback = function()
	  lock = false
  	end    
})
Tab:AddButton({
	Name = "orbita os players",
	Callback = function()
	  loadstring(game:HttpGet("https://raw.githubusercontent.com/0Ben1/fe./main/Orbit%20GUI"))()
  	end    
})

