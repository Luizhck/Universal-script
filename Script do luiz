--muscle simulator--

local Tab = Window:MakeTab({
	Name = "muscle simulator",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddButton({
	Name = "farm",
	Callback = function()
      		cliq = true
      		while true do
      		wait(0)
      		if cliq == true then
      		local args = {
    [1] = game:GetService("Players").LocalPlayer.Character.Weight
}

game:GetService("ReplicatedStorage").Events.WeightMuscle:FireServer(unpack(args))
       end
  	end
  	end    
})
Tab:AddButton({
	Name = "auto rebirth",
	Callback = function()
       while true do
       wait(0)
       local args = {
    [1] = game:GetService("Players").LocalPlayer.Character.Weight
}

game:GetService("ReplicatedStorage").Events.WeightMuscle:FireServer(unpack(args))
end
  	end    
})
Tab:AddButton({
	Name = "farm na barra",
	Callback = function()
      		far = true
      		while true do
      		wait(0)
      		if far == true then
      		game:GetService("ReplicatedStorage").Events.MuscleEvent:FireServer()
end
  	end
  	end    
})
Tab:AddButton({
	Name = "Button!",
	Callback = function()
      	 far = false
  	end    
})
