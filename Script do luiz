      		 dam = true

while true do
wait(0)
if dam == true then
local args = {
    [1] = workspace.Weights.lvl16.Part.LiftProx
}

game:GetService("ReplicatedStorage").Events.Lift:FireServer(unpack(args))
end
end
  	end  
})
